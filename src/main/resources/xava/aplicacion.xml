<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd">

<aplicacion nombre="stud_io2">

	<!--
	Un módulo por defecto se asume para cada
	componente con el controlador Typical
	-->
	<modulo-defecto>
		<controlador nombre="Typical"/>
	</modulo-defecto>

	<!-- Módulo Matricula con controlador personalizado -->
	<modulo nombre="Matricula">
		<modelo nombre="Matricula"/>
		<controlador nombre="MatriculaController"/>
	</modulo>

	<!-- Módulo Asignacion con controlador personalizado -->
	<modulo nombre="Asignacion">
		<modelo nombre="Asignacion"/>
		<controlador nombre="AsignacionController"/>
	</modulo>

</aplicacion>
